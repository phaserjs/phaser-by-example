{
  "name": "starshipped",
  "version": "0.0.1",
  "description": "Starshipped phaser game",
  "main": "server.js",
  "scripts": {
    "start": "npx webpack-cli serve -c webpack.dev.js --mode development",
    "build": "webpack --config webpack.prod.js",
