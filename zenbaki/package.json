{
  "name": "generic",
  "productName": "Generic",
  "main": "index.html",
  "window": {
    "title": "Generic - by Pello",
    "width": 800,
    "height": 600,
    "fullscreen": false,
    "toolbar": false,
    "icon": "customIcon.png"
  },
  "version": "0.0.1",
  "description": "Generic game for jam",
  "chromium-args": "--data-path='./saves/' --in-process-gpu --disable-transparency",
  "scripts": {
    "start": "npx webpack-cli serve -c webpack.dev.js --mode development",
    "build": "webpack --config webpack.prod.js",
    "test": "npm run lint && npm run unit",
    "lint": "eslint src/**/*.js test/**/*.js --fix",
    "unit": "mocha test --recursive --require @babel/register -r jsdom-global/register --file test/test_setup.js",
    "publish": "npm run-script build && npm run-script itchio",
    "itchio": "cd dist && rm -f assets/html/index.html && butler push . pello/zenbaki:html --userversion 0.0.1 && cd .. && butler status pello/zenbaki:html"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "phaser": "^3.80.1",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.10",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^29.6.1",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.2.1",
    "path": "^0.12.7",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "repository": {
    "url": "https://github.com/pxai/phaserbook"
  },
  "license": "MIT",
  "keywords": [
    "phaser",
    "games",
    "book",
    "pello"
  ]
}
